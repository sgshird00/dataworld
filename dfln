[32m+[m[32mwin.title("Menu App")[m
[32m+[m[32mwin.geometry('280x280')[m
[32m+[m[32m#functions[m
[32m+[m[32mdef new():[m
[32m+[m[32m    labl.configure(text="Newfile Clicked")[m
[32m+[m[32m    submenu1.entryconfig('New', state='disabled')[m
[32m+[m[32m#Main Menu Points[m
[32m+[m[32mmenu1 = Menu(win)[m
[32m+[m[32msubmenu1 = Menu(menu1,tearoff=0)[m
