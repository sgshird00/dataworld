[32m+[m
[32m+[m
[32m+[m[32mdef rc():[m
[32m+[m[32m    h = float(hent.get())[m
[32m+[m[32m    l = float(lent.get())[m
[32m+[m[32m    w = float(went.get())[m
[32m+[m[32m    if fent.get() == "":[m
[32m+[m[32m        f = 25.4[m
[32m+[m[32m    else:[m
[32m+[m[32m        f = float(fent.get())[m
[32m+[m[32m    reel.configure(text=f'REEL Size: {(h+w)/f}')[m
[32m+[m[32m    cut.configure(text=f'CUT Size: {(((l+w)*2)+50)/f}')[m
[32m+[m
